{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project" : "./tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/no-namespace": "off",
        "eslint-disable @typescript-eslint/semi":"overrides",
        "comma-dangle":"off"
    }
}
